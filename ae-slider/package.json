{
  "name": "ngx-swiper-wrapper-example",
  "description": "Example app for the ngx-swiper-wrapper",
  "private": true,
  "license": "MIT",
  "version": "7.0.3",
  "tslint": {
    "extends": "./tslint.json"
  },
  "stylelint": {
    "extends": "./stylelint.json"
  },
  "scripts": {
    "start": "ng serve",
    "clean": "rm -rf dist",
    "lint": "npm run lint:ts && npm run lint:css",
    "build": "rm -rf dist/* && npm run build:aot",
    "deploy": "rm -rf dist && deploy-to-git",
    "lint:ts": "tslint -p src/tsconfig.json \"src/**/*.ts\"",
    "lint:css": "stylelint --syntax scss \"src/**/*.css\"",
    "build:aot": "webpack --config config/webpack-build-aot.config.js",
    "build:jit": "webpack --config config/webpack-build-jit.config.js",
    "serve:sjs": "lite-server -c config/lite-server-bs-config.js",
    "serve:wds": "webpack-dev-server --open --inline --config config/webpack-server-jit.config.js"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:zefoy/ngx-swiper-wrapper.git",
      "branch": "gh-pages",
      "folder": "dist",
      "script": "npm run build",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "ZEF Devel",
        "email": "devel@zef.fi"
      }
    }
  },
  "dependencies": {
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "core-js": "^2.5.7",
    "ngx-swiper-wrapper": "^7.0.0",
    "rxjs": "^6.3.0",
    "swiper": "^4.4.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.5",
    "@angular-devkit/build-ng-packagr": "^0.10.5",
    "@angular/cli": "^7.0.5",
    "@angular/compiler-cli": "^7.0.3",
    "@ngtools/webpack": "^7.0.5",
    "@types/node": "^10.12.5",
    "@types/swiper": "^4.2.4",
    "codelyzer": "^4.5.0",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "deploy-to-git": "^0.1.5",
    "html-webpack-plugin": "^3.2.0",
    "lite-server": "^2.4.0",
    "ng-packagr": "^4.4.0",
    "plugin-typescript": "^8.0.0",
    "raw-loader": "^0.5.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "systemjs": "^2.1.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
